{
	"name": "@mypreview/woo-additional-terms",
	"version": "1.3.5",
	"private": true,
	"description": "Add additional terms and condition checkbox to the WooCommerce checkout.",
	"homepage": "https://github.com/mypreview/woo-additional-terms#readme",
	"bugs": {
		"url": "https://github.com/mypreview/woo-additional-terms/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mypreview/woo-additional-terms.git"
	},
	"license": "GPL-3.0-or-later",
	"author": "Mahdi Yazdani",
	"scripts": {
		"archive": "npm run pre-archive && rm -rf $npm_package_name && composer archive --file=$npm_package_name --format=zip",
		"format": "prettier --ignore-path .prettierignore --write \"**/*.{js,jsx,json,ts,tsx}\"",
		"lint:js": "eslint assets --ext=js,jsx,json,ts,tsx",
		"lint:js-fix": "eslint assets --ext=js,jsx,json,ts,tsx --fix",
		"lint:php": "composer lint:php",
		"lint:wpcs": "composer lint:wpcs",
		"lint:wpcs-fix": "composer lint:wpcs-fix",
		"makepot": "composer make-pot",
		"pre-archive": "npm run format && npm run lint:wpcs-fix"
	},
	"devDependencies": {
		"@babel/core": "7.19.1",
		"@wordpress/eslint-plugin": "13.1.0",
		"@wordpress/prettier-config": "2.0.0",
		"lint-staged": "13.0.3",
		"prettier": "npm:wp-prettier@^2.6.2",
		"wp-prettier": "2.6.2"
	}
}
